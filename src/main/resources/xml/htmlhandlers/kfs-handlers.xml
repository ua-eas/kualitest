<handler>
    <application>KFS</application>
    <tag-name>input</tag-name>
    <handler-class-name>org.kuali.test.handlers.HiddenInputTagHandler</handler-class-name>
    <tag-matchers>
        <tag-matcher>
            <match-type>current</match-type>
            <tag-name>input</tag-name>
            <match-attributes>
                <match-attribute>
                    <name>type</name>
                    <value>hidden</value>
                </match-attribute>
                <match-attribute>
                    <name>name</name>
                    <!--  can use "|" to seperate "or" values for matching -->
                    <value>docNum|businessObjectClassName</value>
                </match-attribute>
            </match-attributes>
        </tag-matcher>
    </tag-matchers>
</handler>
