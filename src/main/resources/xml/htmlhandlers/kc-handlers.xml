<?xml version="1.0" encoding="UTF-8"?>
<tag-handlers xmlns="test.kuali.org">
    <handler>
        <tag-name>td</tag-name>
        <handler-class-name>org.kuali.test.handlers.TdTagHandler</handler-class-name>
        <tag-matchers>
            <tag-matcher>
                <match-type>current</match-type>
                <tag-name>td</tag-name>
                <match-attributes></match-attributes>
            </tag-matcher>
            <tag-matcher>
                <match-type>parent</match-type>
                <tag-name>div</tag-name>
                <search-definition>*</search-definition>
                <match-attributes>
                    <match-attribute>
                        <name>class</name>
                        <value>tab-container</value>
                    </match-attribute>
                </match-attributes>
            </tag-matcher>
            <tag-matcher>
                <match-type>sibling</match-type>
                <tag-name>th</tag-name>
                <search-definition>-1</search-definition>
                <match-attributes></match-attributes>
            </tag-matcher>
        </tag-matchers>
        <label-matcher>
            <tag-matcher>
                <match-type>sibling</match-type>
                <tag-name>th</tag-name>
                <search-definition>-1</search-definition>
                <match-attributes></match-attributes>
            </tag-matcher>
        </label-matcher>
        <section-matcher>
            <tag-matcher>
                <match-type>parent</match-type>
                <tag-name>table</tag-name>
                <search-definition>*</search-definition>
                <match-attributes></match-attributes>
            </tag-matcher>
            <tag-matcher>
                <match-type>sibling</match-type>
                <tag-name>h3|h2</tag-name>
                <search-definition>-1</search-definition>
                <match-attributes></match-attributes>
            </tag-matcher>
            <tag-matcher>
                <match-type>child</match-type>
                <tag-name>span</tag-name>
                <search-definition>1</search-definition>
                <match-attributes>
                    <match-attribute>
                        <name>class</name>
                        <value>subhead-left</value>
                    </match-attribute>
                </match-attributes>
            </tag-matcher>
        </section-matcher>
        <handler-name>kc1</handler-name>
    </handler>
</tag-handlers>