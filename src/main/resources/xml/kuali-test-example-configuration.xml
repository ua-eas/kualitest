<?xml version="1.0" encoding="UTF-8"?>
<kuali-test-configuration xmlns="test.kuali.org">
   <repository-location>/mypath/kuali-test-repository</repository-location>
   <test-result-location>/mypath/kuali-test-repository/kuali-test-results</test-result-location>
   <tag-handlers-location>/mypath/kuali-test-repository/kuali-test-repository/xml/htmlhandlers</tag-handlers-location>
   <default-test-wait-interval>2</default-test-wait-interval>
   <additional-db-info-location>/mypath/kuali-test-repository/kuali-test-repository/xml/additional-db-info</additional-db-info-location>
   <database-connections>
      <database-connection>
         <type>Oracle</type>
         <name>kfs dev</name>
         <jdbc-url>myurl</jdbc-url>
         <jdbc-driver>oracle.jdbc.driver.OracleDriver</jdbc-driver>
         <schema>myschema</schema>
         <username>myuser</username>
         <password>mypassword</password>
         <configured-tables-only>true</configured-tables-only>
      </database-connection>
   </database-connections>
   <web-services/>
   <jmx-connections/>
   <modified>false</modified>
   <email-setup>
      <mail-host>myemailhost</mail-host>
      <subject>kuali test results</subject>
      <from-address>myemail</from-address>
      <to-addresses>myemail</to-addresses>
   </email-setup>
   <platforms>
      <platform>
         <name>kfs dev</name>
         <application>KFS</application>
         <version>3</version>
         <web-url>myurl</web-url>
         <web-service-name/>
         <test-suites/>
         <platform-tests>
         </platform-tests>
      </platform>
   </platforms>
   <parameters-requiring-encryption>
      <name>password</name>
   </parameters-requiring-encryption>
   <encryption-password-file>/mypath/kuali-test-repository/pass.txt</encryption-password-file>
   <auto-replace-parameters>
      <auto-replace-parameter>
         <parameter-name>lt</parameter-name>
         <tag-name>input</tag-name>
         <tag-attributes>
            <attribute>
               <name>type</name>
               <value>hidden</value>
            </attribute>
         </tag-attributes>
         <from-input-parameter>false</from-input-parameter>
      </auto-replace-parameter>
      <auto-replace-parameter>
         <parameter-name>ticket</parameter-name>
         <tag-name>ticket</tag-name>
         <tag-attributes/>
         <from-input-parameter>true</from-input-parameter>
      </auto-replace-parameter>
   </auto-replace-parameters>
   <parameters-to-ignore/>
   <urls-to-ignore>
      <url>*/RouteLog.do?routeHeaderId=*</url>
      <url>*.dwr</url>
   </urls-to-ignore>
   <referrers-to-ignore>
      <referrer>*/portal.do?channelTitle=Contract%20Manager%20Assignment*</referrer>
   </referrers-to-ignore>
   <default-web-service-polling>true</default-web-service-polling>
   <date-replace-format>MM/dd/yyyy</date-replace-format>
   <auto-update-windows-registry-for-proxy>true</auto-update-windows-registry-for-proxy>
   <hosts-requiring-https>
       <host>somehost</host>
   </hosts-requiring-https>
   <output-ignored-results>true</output-ignored-results>
   <http-pre-submit-processors/>
   <http-post-submit-processors/>
   <error-indicators>
	   <indicator>Error</indicator>
   </error-indicators>
   <pdf-viewer-path>acroread</pdf-viewer-path>
   <form-submit-element-names>
	   <element-name>methodToCall.submit</element-name>
	   <element-name>methodToCall.calculate</element-name>
	   <element-name>methodToCall.reload</element-name>
   </form-submit-element-names>
</kuali-test-configuration>
