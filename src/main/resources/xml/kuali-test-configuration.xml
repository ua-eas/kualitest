    <tag-handlers>
        <handler>
            <tag-name>input</tag-name>
            <handler-class-name>org.kuali.test.handlers.TextInputTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>input</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>type</name>
                            <value>text</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>div</tag-name>
            <handler-class-name>org.kuali.test.handlers.KualiWorkareaTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>div</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>class</name>
                            <value>workarea</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>td</tag-name>
            <handler-class-name>org.kuali.test.handlers.TdTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>td</tag-name>
                    <match-attributes>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>input</tag-name>
            <handler-class-name>org.kuali.test.handlers.RadioInputTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>input</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>type</name>
                            <value>radio</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>input</tag-name>
            <handler-class-name>org.kuali.test.handlers.CheckboxInputTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>input</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>type</name>
                            <value>checkbox</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>input</tag-name>
            <handler-class-name>org.kuali.test.handlers.HiddenInputTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>input</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>type</name>
                            <value>hidden</value>
                        </match-attribute>
                        <match-attribute>
                            <name>name</name>
                            <!--  can use "|" to seperate "or" values for matching -->
                            <value>docNum|businessObjectClassName</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>select</tag-name>
            <handler-class-name>org.kuali.test.handlers.SelectInputTagHandler</handler-class-name>
        </handler>
        <handler>
            <tag-name>table</tag-name>
            <handler-class-name>org.kuali.test.handlers.KualiHeaderBoxTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>table</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>class</name>
                            <value>headerinfo</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>table</tag-name>
            <handler-class-name>org.kuali.test.handlers.KualiTabTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>table</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>class</name>
                            <value>tab</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
            <label-matcher>
                <match-type>child</match-type>
                <!-- comma delimited list indicates pare/child relationships -->
                <tag-name>tbody,tr,td,h2</tag-name>
                <match-attributes></match-attributes>
            </label-matcher>
        </handler>
        <handler>
            <tag-name>div</tag-name>
            <handler-class-name>org.kuali.test.handlers.KualiWorkareaTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>current</match-type>
                    <tag-name>div</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>class</name>
                            <value>workarea</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
        </handler>
        <handler>
            <tag-name>td</tag-name>
            <handler-class-name>org.kuali.test.handlers.KualiHeaderInfoCellTagHandler</handler-class-name>
            <tag-matchers>
                <tag-matcher>
                    <match-type>parent</match-type>
                    <tag-name>table</tag-name>
                    <match-attributes>
                        <match-attribute>
                            <name>class</name>
                            <value>headerinfo</value>
                        </match-attribute>
                    </match-attributes>
                </tag-matcher>
            </tag-matchers>
            <label-matcher>
                <match-type>previous-sibling</match-type>
                <tag-name>th</tag-name>
                <match-attributes></match-attributes>
            </label-matcher>
        </handler>
    </tag-handlers>
